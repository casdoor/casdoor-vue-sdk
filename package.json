{
  "name": "casdoor-vue-sdk",
  "version": "1.2.0",
  "description": "Vue client SDK for Casdoor",
  "private": false,
  "license": "Apache-2.0",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "tsc && babel src --out-dir dist/src && babel index.js --out-dir dist/ && cp package.json dist/ && cp LICENSE dist/ && cp README.md dist/",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "casdoor-js-sdk": "^0.2.7",
    "core-js": "^3.8.3",
    "vue-demi": "latest"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.3.0",
    "vue": "^2.0.0 || >=3.0.0-rc.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.3",
    "@semantic-release/npm": "^7.1.3",
    "@semantic-release/release-notes-generator": "^9.0.3",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "semantic-release": "^17.4.4",
    "typescript": "^4.8.4",
    "vue": "^3.0.0",
    "webpack-cli": "^4.9.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "keywords": [
    "auth",
    "authn",
    "authentication",
    "sso",
    "oauth",
    "oidc",
    "casbin",
    "casdoor"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/casdoor/casdoor-vue-sdk.git"
  },
  "author": {
    "name": "Nekotoxin",
    "email": "Nekotoxin@outlook.com"
  },
  "bugs": {
    "url": "https://github.com/casdoor/casdoor-vue-sdk/issues"
  },
  "homepage": "https://github.com/casdoor/casdoor-vue-sdk"
}
